# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Calendar Integration (Required for real calendar data)
VITE_GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here
VITE_GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret_here

# Airtable Integration (Optional - for additional data sources)
VITE_AIRTABLE_PERSONAL_TOKEN=your_airtable_personal_token_here
VITE_AIRTABLE_BASE_ID=your_airtable_base_id_here

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Debug Mode (Optional)
VITE_DEBUG_CALENDAR=false

# App Configuration
VITE_APP_NAME="Nindra Chatbot System"
VITE_APP_VERSION="1.0.0"

# Environment Configuration
NODE_ENV=development 